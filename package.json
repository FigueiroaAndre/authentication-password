  "bugs": {
    "url": "https://github.com/FigueiroaAndre/authentication-password/issues"
  },
  "homepage": "https://github.com/FigueiroaAndre/authentication-password#readme",
  "devDependencies": {
    "jest": "^25.3.0",
    "jest-cli": "^25.3.0",
    "nodemon": "^2.0.3"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1"
  }
